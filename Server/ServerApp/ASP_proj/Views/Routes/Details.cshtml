@model ASP_proj.Models.Route

@{
    ViewData["Title"] = "Route details";
}

<h2>Route details</h2>

<dl class="row">
    <dt class="col-sm-2">Id</dt>
    <dd class="col-sm-10">@Model.RouteId</dd>

    <dt class="col-sm-2">Number</dt>
    <dd class="col-sm-10">@Model.Number</dd>

    <dt class="col-sm-2">Start</dt>
    <dd class="col-sm-10">@Model.StartPoint</dd>

    <dt class="col-sm-2">End</dt>
    <dd class="col-sm-10">@Model.EndPoint</dd>
</dl>

<a href="/Routes/Index" class="btn btn-secondary">Back</a>
<a href="/Routes/Edit/@Model.RouteId" class="btn btn-warning">Edit</a>