@model ASP_proj.Models.Schedule

@{
    ViewData["Title"] = "Schedule details";
}

<h2>Schedule details</h2>

<dl class="row">
    <dt class="col-sm-2">Id</dt>
    <dd class="col-sm-10">@Model.ScheduleId</dd>

    <dt class="col-sm-2">Driver Id</dt>
    <dd class="col-sm-10">@Model.DriverId</dd>

    <dt class="col-sm-2">Route Id</dt>
    <dd class="col-sm-10">@Model.RouteId</dd>

    <dt class="col-sm-2">Departure</dt>
    <dd class="col-sm-10">@Model.DepartureTime</dd>

    <dt class="col-sm-2">Status</dt>
    <dd class="col-sm-10">@Model.Status</dd>
</dl>

<a href="/Schedules/Index" class="btn btn-secondary">Back</a>
<a href="/Schedules/Edit/@Model.ScheduleId" class="btn btn-warning">Edit</a>